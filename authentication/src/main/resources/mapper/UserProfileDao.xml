<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.haiyinlong.snail.authentication.infrastructure.dao.UserProfileDao">
    <select id="getByUserId"
            resultType="UserProfileEntity">
        select *
        from user_profile
        where user_id = #{userId}
    </select>
</mapper>
